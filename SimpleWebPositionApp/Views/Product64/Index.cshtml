@model SimpleWebPositionApp.Models.SearchBar
@{
    ViewData["Title"] = "Αρχική Σελίδα";
}
<h4>Aναζήτηση Προϊόντος στο 64</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post" asp-controller="Product64" asp-action="Search">
            <div class="form-group">
                <label asp-for="Code" class="control-label">Aναζήτηση</label>
                <input asp-for="Code" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Αναζήτηση" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

