@model SimpleWebPositionApp.Models.UploadFile
@{
    ViewData["Title"] = "Πρόσθεση Αρχείου";
}
<h4>ProductFile</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Upload" method="post" enctype="multipart/form-data">
            
            <div class="form-group">
                <label asp-for="ExcelFile" class="control-label">Αρχείο Εxcel</label>
                <input asp-for="ExcelFile" class="form-control" type="file"/>
                <label asp-for="SelWarehouse" class="control-label">Επιθυμητή Αποθήκη</label>
                <select asp-for="SelWarehouse" class="form-select">
                    <option value="68">ΑΛΚΜΗΝΗΣ 68</option>
                    <option value="64">ΑΛΚΜΗΝΗΣ 64</option>
                </select>
            </div>
            <div class="form-group">
                <input type="submit" value="Upload" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

